   fetchSearch(res);\r\n        }\r\n        setIsLoading(false);\r\n      })\r\n      .catch((e) => {\r\n        setErrors(e.response.data.errors);\r\n        setIsLoading(false);\r\n      });\r\n  }\r\n\r\n  function fetchSearch(res) {\r\n    page > 1\r\n      ? setImages([...images, ...res.data.results])\r\n      : setImages([...res.data.results]);\r\n  }\r\n\r\n  function fetchRandom(res) {\r\n    page > 1 ? setImages([...images, ...res.data]) : setImages([...res.data]);\r\n  }\r\n\r\n  useEffect(() => {\r\n    setIsLoading(true);\r\n    fetch();\r\n  }, [page, searchTerm]);\r\n\r\n  return [images, setImages, errors, isLoading];\r\n}\r\n","D:\\React\\my-react\\src\\components\\Loading.js",[],"D:\\React\\my-react\\src\\utils\\hooks\\useDebounce.js",[],"D:\\React\\my-react\\src\\page\\Home.js",[],"D:\\React\\my-react\\src\\page\\Login.js",[],"D:\\React\\my-react\\src\\page\\Gallery.js",[],"D:\\React\\my-react\\src\\components\\Header.js",["107"],"import React, { useContext } from \"react\";\r\nimport { NavLink, useHistory } from \"react-router-dom\";\r\nimport firebase from \"../config/firebase\";\r\nimport AppContext from \"../store/AppContext\";\r\n\r\nexport default function Header() {\r\n  const [isLoggedIn, user] = useContext(AppContext);\r\n  const history = useHistory();\r\n\r\n  function logout() {\r\n    firebase\r\n      .auth()\r\n      .signOut()\r\n      .then((res) => {\r\n        history.push(\"/\");\r\n      })\r\n      .catch((e) => {\r\n        console.log(e.response.data);\r\n      });\r\n  }\r\n\r\n  return (\r\n    <nav className=\"py-5 bg-gray-900 text-white flex justify-between\">\r\n      <ul className=\"flex justify-between px-10\">\r\n        <li className=\"mr-5\">\r\n          <NavLink to=\"/\" exact activeClassName=\"underline text-blue-200\">\r\n            Home\r\n          </NavLink>\r\n        </li>\r\n        <li className=\"mr-5\">\r\n          <NavLink to=\"/gallery\" activeClassName=\"underline text-blue-200\">\r\n            